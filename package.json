{
  "name": "pure-react",
  "version": "1.0.0",
  "main": "index.js",
  "author": "gyang",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*{js, jsx, css, json, html} \"",
    "format:check": "prettier --list-different \"src/**/*.{js, jsx, css, json, html}\"",
    "lint": "eslint \"src**/*.{js,jsx}\"",
    "dev": "parcel src/index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run format:check"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.0.0",
    "react-dom": "^16.6.0"
  }
}
